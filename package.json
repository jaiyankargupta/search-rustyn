{
    "name": "search-rustyn",
    "version": "1.0.0",
    "homepage": "https://jaiyankargupta.github.io/searchRustyn",
    "private": true,
    "dependencies": {
        "autoprefixer": "^10.4.14",
        "concurrently": "^9.2.0",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "http-proxy-middleware": "^2.0.6",
        "lucide-react": "^0.263.1",
        "next": "^14.0.0",
        "postcss": "^8.4.24",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "tailwindcss": "^3.3.0"
    },
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "server": "node server.js",
        "dev:full": "concurrently \"npm run server\" \"npm run dev\"",
        "deploy": "npm run build:export && gh-pages -d out",
        "build:export": "cp next.config.production.js next.config.js && next build && cp next.config.js next.config.production.js",
        "start:prod": "NODE_ENV=production node server.js",
        "lint": "next lint"
    },
    "eslintConfig": {
        "extends": [
            "next/core-web-vitals"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "eslint": "^9.33.0",
        "gh-pages": "^6.3.0"
    }
}