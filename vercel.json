{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://what-to-build.niladri.tech/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "env": {
        "REACT_APP_API_BASE_URL": "https://what-to-build.niladri.tech"
    }
}